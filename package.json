{
  "name": "ismailmmd.github.io",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "dev": "next dev",
    "build": "next build --no-lint",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.24.0",
    "@chakra-ui/theme-tools": "^2.2.6",
